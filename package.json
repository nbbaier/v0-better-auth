{
	"name": "vite-auth-app",
	"version": "0.0.0",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "tsc && vite build",
		"dev": "vite",
		"dev:server": "bun run --watch server/index.ts",
		"preview": "vite preview",
		"typecheck": "tsc --noEmit",
		"db:generate": "bunx --bun @better-auth/cli generate",
		"db:migrate": "bunx --bun  @better-auth/cli migrate"
	},
	"dependencies": {
		"@hono/node-server": "^1.19.5",
		"@radix-ui/react-avatar": "latest",
		"@radix-ui/react-tabs": "latest",
		"@types/better-sqlite3": "^7.6.13",
		"better-auth": "^1.3.27",
		"better-sqlite3": "^12.4.1",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"date-fns": "latest",
		"hono": "^4.9.10",
		"input-otp": "latest",
		"lucide-react": "^0.454.0",
		"react": "^19",
		"react-dom": "^19",
		"tailwind-merge": "^2.5.5",
		"tailwindcss-animate": "^1.0.7",
		"tw-animate-css": "^1.4.0"
	},
	"devDependencies": {
		"@tailwindcss/vite": "^4.1.14",
		"@types/node": "^24.7.0",
		"@types/react": "^19.2.2",
		"@types/react-dom": "^19",
		"@vitejs/plugin-react": "^4.3.4",
		"tailwindcss": "^4.1.14",
		"tsx": "^4.20.6",
		"typescript": "^5",
		"vite": "^6.0.11",
		"@types/bun": "latest"
	}
